  <default_xs>71c</default_xs>

  <!-- UO2F2/D2O Solution -->
  <material id="1">
    <density units="sum" />
    <nuclide name="U-234" ao="1.9029e-05" />
    <nuclide name="U-235" ao="1.7397e-03" />
    <nuclide name="U-238" ao="9.7761e-05" />
    <nuclide name="F-19"  ao="3.7129e-03" />
    <element name="O"     ao="3.3461e-02" />
    <nuclide name="H-2"   ao="5.9318e-02" />
    <nuclide name="H-1"   ao="1.7849e-04" />
    <sab name="HH2O" xs="71t" />
    <sab name="hwtr" xs="71t" />
  </material>

  <!-- 321 Stainless Steel -->
  <material id="2">
    <density units="sum" />
    <element name="Fe" ao="5.9355e-02" />
    <element name="Cr" ao="1.6511e-02" />
    <element name="Ni" ao="7.7203e-03" />
    <element name="Mn" ao="1.7363e-03" />
    <element name="Si" ao="1.6982e-03" />
  </material>

  <!-- Heavy Water -->
  <material id="3">
    <density units="sum" />
    <nuclide name="H-2" ao="6.6078e-02" />
    <nuclide name="H-1" ao="3.9886e-04" />
    <element name="O"   ao="3.3238e-02" />
    <sab name="HH2O" xs="71t" />
    <sab name="hwtr" xs="71t" />
  </material>

</materials>
