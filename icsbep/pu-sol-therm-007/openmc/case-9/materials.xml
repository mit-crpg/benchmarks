  -->

  <default_xs>70c</default_xs>

  <!-- Plutonium nitrate solution -->
  <material id="1">
    <density units="sum" />
    <nuclide name="Pu-238" ao="1.5421e-08" />
    <nuclide name="Pu-239" ao="2.4318e-04" />
    <nuclide name="Pu-240" ao="1.1898e-05" />
    <nuclide name="Pu-241" ao="7.7414e-07" />
    <nuclide name="Pu-242" ao="2.2749e-08" />
    <nuclide name="N-14"   ao="1.3850e-03" />
    <nuclide name="H-1"    ao="6.3152e-02" />
    <nuclide name="O-16"   ao="3.5550e-02" />
    <sab name="lwtr" xs="10t" />
  </material>

  <!-- 304L stainless steel -->
  <material id="2">
    <density units="sum" />
    <element name="Fe" ao="5.9355e-02" />
    <element name="Cr" ao="1.7428e-02" />
    <element name="Ni" ao="7.7203e-03" />
    <element name="Mn" ao="1.7363e-03" />
  </material>

  <!-- Water at 25 C -->
  <material id="3">
    <density units="sum" />
    <nuclide name="H-1"  ao="6.6655e-02" />
    <nuclide name="O-16" ao="3.3327e-02" />
    <sab name="lwtr" xs="10t" />
  </material>

</materials>
